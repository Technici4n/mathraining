<%= render 'shared/error_messages', object: f.object %>

<div class="form-group">
<%= f.label :first_name %>
<%= f.text_field :first_name, :class => "form-control", :style => "width:200px;", :disabled => current_user.other %>
</div>

<div class="form-group">
<%= f.label :last_name %>
<%= f.text_field :last_name, :class => "form-control", :style => "width:200px;", :disabled => current_user.other %>
</div>

<div class="form-group">
<%= f.label :email %>
<%= f.email_field :email, :class => "form-control", :style => "width:200px;", :disabled => current_user.other %>
</div>

<div class="form-group">
<%= f.label :password %>
<%= f.password_field :password, :class => "form-control", :style => "width:200px;", :disabled => current_user.other %>
</div>

<div class="form-group">
<%= f.label :password_confirmation %>
<%= f.password_field :password_confirmation, :class => "form-control", :style => "width:200px;", :disabled => current_user.other %>
</div>
